<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<threadsafe>true</threadsafe>
	<automatic-scaling>
		<min-idle-instances>5</min-idle-instances>

		<!-- ‘automatic’ is the default value. -->
		<max-idle-instances>10</max-idle-instances>

		<!-- ‘automatic’ is the default value. -->
		<min-pending-latency>10ms</min-pending-latency>
		<max-pending-latency>30ms</max-pending-latency>

		<!-- ‘8’ is the default value. -->
		<max-concurrent-requests>10</max-concurrent-requests>
	</automatic-scaling>
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	</system-properties>
	<env-variables>
		<env-var name="TWILIO_ACCOUNT_SID" value="" />
		<env-var name="TWILIO_AUTH_TOKEN" value="" />
		<env-var name="TWILIO_NUMBER" value="" />
		<env-var name="GOOGLE_MAPS_DIRECTION_API_KEY" value="" />
		<env-var name="GOOGLE_MAPS_PLACES_API_KEY" value="" />

		<env-var name="TWILIO_ASU_ACCOUNT_SID_TEST" value="" />
		<env-var name="TWILIO_ASU_AUTH_TOKEN_TEST" value="" />
		<env-var name="TWILIO_ASU_MAGIC_NUMBER" value="" />

		<env-var name="TWILIO_ASU_ACCOUNT_SID_LIVE" value="" />
		<env-var name="TWILIO_ASU_AUTH_TOKEN_LIVE" value="" />
		<env-var name="TWILIO_ASU_NUMBER" value="" />

	</env-variables>
</appengine-web-app>
